                                    "gender",
                                    "address",
                                    "phone_no"
                                ],
                                "properties": {
                                    "age": {
                                        "type": "integer",
                                        "example": 30
                                    },
                                    "gender": {
                                        "type": "string",
                                        "example": "male"
                                    },
                                    "address": {
                                        "type": "string",
                                        "example": "123 Main St"
                                    },
                                    "phone_no": {
                                        "type": "string",
                                        "example": "1234567890"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Patient created successfully"
                    },
                    "400": {
                        "description": "Validation errors"
